{
    "name": "laravel-code/crud",
    "description": "rest api crud plugin for laravel 6/7",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Raymond van Vuuren",
            "email": "rsvvuuren@hotmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^8.49",
        "orchestra/testbench": "^6.19"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "guzzlehttp/guzzle": "^6.5",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "facade/ignition": "^2.0.1",
        "psalm/phar": "^3.9"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelCode\\Crud\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "LaravelCode\\Crud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests\\TestApp"
        }
    },
    "scripts": {
        "fix": [
            "php-cs-fixer fix"
        ],
        "test": [
            "phpunit"
        ]
    }
}
